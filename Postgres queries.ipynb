   "source": [
    "PostgreSQL\n",
    "\n",
    "postgres -rdbms - service running on your machine\n",
    "\n",
    "start/stop db: pg-start pg-stop\n",
    "is it running? sudo brew services list\n",
    "\n",
    "PSQL - cli repl to access postgresql instance(service, server)\n",
    "\n",
    "PostGres specific\n",
    "\\q - quit\n",
    "\\h - help\n",
    "\\? - other backslash commands\n",
    "\n",
    "list databases:\n",
    "\\l\n",
    "\n",
    "useful datatypes: boolean, varchar, timestamp (not date), integer, json (specific to postgres, not in all db's), text(store strings and dont know length), \n",
    "\n",
    "manage users:\n",
    "\\du - list users\n",
    "\\h CREATE USER <-- help on how to create user\n",
    "CREATE USER batman WITH ENCRYPTED PASSWORD 'robin';\n",
    "\n",
    "create database:\n",
    "\\h CREATE DATABASE\n",
    "CREATE DATABASE batcave WITH OWNER batman;\n",
    "\n",
    "connect to database:\n",
    "the psql prompt shows current db\n",
    "\\c batcave < -- connects to batcave db\n",
    "\n",
    "tables commands (also called relations):\n",
    "\\dt - list the tables\n",
    "make a new table\n",
    "    idea: create a table named movies with:\n",
    "        primary key, id\n",
    "        title VARCHAR(30)\n",
    "        year INT\n",
    "        genre VARCHAR(30)\n",
    "  syntax:\n",
    "    CREATE TABLE movies (\n",
    "    id SERIAL NOT NULL PRIMARY KEY,\n",
    "    title VARCHAR(30),\n",
    "    year INT,\n",
    "    genre VARCHAR(15),\n",
    "    );\n",
    "    \n",
    "inspect tables:\n",
    "\\dt - list tables\n",
    "\\d movies - inspect the table\n",
    "\n",
    "update table:\n",
    "\\h ALTER TABLE\n",
    "ALTER TABLE movies ADD COLUMN year INT\n",
    "\n",
    "add rows:\n",
    "INSERT INTO movies (title, year, genre) \n",
    "VALUES ('batman begins', 2005, 'superhero');\n",
    "\n",
    "query databse:\n",
    "SELECT * FROM movies;\n",
    "\n",
    "\n",
    "create/drop user:\n",
    "CREATE USER robin;\n",
    "DROP USER IF EXISTS robin;\n",
    "\n",
    "\n",
    "select NOW(); <--- current db time\n",
    "select * from movies where year < now(); \n",
    "\n",
    "\n",
    "CREATE TABLES books (\n",
    "id SERIAL PRIMARY KEY,\n",
    "title VARCHAR(45) NOT NULL,\n",
    "released timestamp without time zone NOT NULL DEFAULT NOW(),\n",
    "genre VARCHAR(45) DEFAULT NULL\n",
    ");\n",
    "\n",
    "alias:\n",
    "SELECT title \"Title\", year \"Year\", genre \"Genre\" from movies; \n",
    "SELECT title t FROM movies\n",
    "\n",
    "INSERT INTO books (title, genre) VALUES\n",
    "('Steven King', 'comedy'),\n",
    "('50 shades', 'comedy'),\n",
    "('Giving Tree', 'childrens');\n",
    "\n"
   ]
  },
